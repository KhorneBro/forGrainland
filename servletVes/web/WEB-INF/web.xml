<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <welcome-file-list>
        <welcome-file>Start.jsp</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>web3</servlet-name>
        <servlet-class>bean.web3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>web3</servlet-name>
        <url-pattern>/bean.web3</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>fName</servlet-name>
        <servlet-class>fName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fName</servlet-name>
        <url-pattern>/fName</url-pattern>
    </servlet-mapping>



</web-app>